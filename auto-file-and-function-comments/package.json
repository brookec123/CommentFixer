{
  "name": "auto-commenter",
  "displayName": "Auto Commenter",
  "description": "This extension will allow you to add comments to your Java, C/C++ (and header), and python files.",
  "publisher": "brookec",
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Formatters", "Other"
  ],
  "activationEvents": ["auto-commenter.comment-writer"],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
        "properties": {
            "brookec.auto-commenter.comment-writer.author": {
                "type": "string",
                "default": "name",
                "description": "Author of file."
            }
        },
        "title": "Auto Commenter"
    },
    "commands": [{
      "command": "auto-commenter.comment-writer",
      "title": "Comment Writer"
    }],
    "keybindings": [
      {
        "command": "auto-commenter.comment-writer",
        "key": "alt+W", 
        "mac": "alt+W"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brookec123/CommentFixer.git"
  }
}